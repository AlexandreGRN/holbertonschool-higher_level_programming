# ---------- base --------- #~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
id is normal int
id is None
too few argument
too many argument
id wrong type = number string ("12")
id wrong type = number string ("hello")
id wrong type = tuple
id wrong type = dictionnary

# ---------- rectangle --------- #~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
                        # ~~~~~~~~~~ Rectangle creation ~~~~~~~~~~ #
# ---------- Normal:
>(different width + height) + (no coordinate) + (No id)
(width = 5, height = 7, x=0, y=0, id=None)

>(different width + height) + (no coordinate) + (id)
(width = 5, height = 7, x=0, y=0, id=50)

>(different width + height) + (coordinate) + (No id)
(width = 5, height = 7, x=2, y=0, id=None)
(width = 5, height = 7, x=0, y=2, id=None)
(width = 5, height = 7, x=2, y=3, id=None)
(width = 5, height = 7, x=2, y=2, id=None)

>(different width + height) + (coordinate) + (id)
(width = 5, height = 7, x=2, y=0, id=50)
(width = 5, height = 7, x=0, y=2, id=50)
(width = 5, height = 7, x=2, y=3, id=50)
(width = 5, height = 7, x=2, y=2, id=50)
#####
>(same width + height) + (no coordinate) + (No id)
(width = 5, height = 5, x=0, y=0, id=None)

>(same width + height) + (no coordinate) + (id)
(width = 5, height = 5, x=0, y=0, id=50)

>(same width + height) + (coordinate) + (No id)
(width = 5, height = 5, x=2, y=0, id=None)
(width = 5, height = 5, x=0, y=2, id=None)
(width = 5, height = 5, x=2, y=3, id=None)
(width = 5, height = 5, x=2, y=2, id=None)

>(same width + height) + (coordinate) + (id)
(width = 5, height = 5, x=2, y=0, id=50)
(width = 5, height = 5, x=0, y=2, id=50)
(width = 5, height = 5, x=2, y=3, id=50)
(width = 5, height = 5, x=2, y=2, id=50)

# ---------- Error:
""" width/height Error"""
# good type
>(width error) + (no coordinate) + (no id)
(width = 0, height = 5, x=0, y=0, id=None)
(width = -1, height = 5, x=0, y=0, id=None)

>(height error) + (no coordinate) + (no id)
(width = 5, height = 0, x=0, y=0, id=None)
(width = 5, height = -1, x=0, y=0, id=None)

>(both error) + (no coordinate) + (no id)
(width = 0, height = 0, x=0, y=0, id=None)
(width = 0, height = -1, x=0, y=0, id=None)
(width = -1, height = -1, x=0, y=0, id=None)
(width = -1, height = 0, x=0, y=0, id=None)

--

>(width error) + (coordinate) + (no id)
(width = 0, height = 5, x=2, y=0, id=None)
(width = -1, height = 5, x=0, y=2, id=None)
(width = -1, height = 5, x=2, y=2, id=None)

>(height error) + (coordinate) + (no id)
(width = 5, height = 0, x=2, y=0, id=None)
(width = 5, height = -1, x=0, y=2, id=None)
(width = 5, height = 0, x=2, y=2, id=None)

>(both error) + (coordinate) + (no id)
(width = 0, height = 0, x=1, y=0, id=None)
(width = 0, height = -1, x=0, y=1, id=None)
(width = -1, height = -1, x=1, y=1, id=None)
(width = -1, height = 0, x=1, y=1, id=None)

--

>(width error) + (coordinate) + (id)
(width = 0, height = 5, x=2, y=0, id=50)

>(height error) + (no coordinate) + (no id)
(width = 5, height = 0, x=0, y=0, id=50)

>(both error) + (coordinate) + (id)
(width = 0, height = 0, x=1, y=0, id=50)
# bad type
>(width error) + (no coordinate) + (no id)
(width = "14", height = 5, x=0, y=0, id=None)
(width = (10,20), height = 5, x=0, y=0, id=None)

>(height error) + (no coordinate) + (no id)
(width = 5, height = "14", x=0, y=0, id=None)
(width = 5, height = (10,20), x=0, y=0, id=None)

>(both error) + (no coordinate) + (no id)
(width = "14", height = "14", x=0, y=0, id=None)
(width = "14", height = (10,20), x=0, y=0, id=None)
(width = (10,20), height = (10,20), x=0, y=0, id=None)
(width = (10,20), height = "14", x=0, y=0, id=None)

""" coordinate Error"""
# good type
>(normal width/height) + (x error) + (id/no id)
(width = 2, height = 3, x=-1, y=0, id=None)
(width = 2, height = 3, x=-1, y=1, id=50)

>(normal width/height) + (y error) + (id/no id)
(width = 2, height = 3, x=0, y=-1, id=None)
(width = 2, height = 3, x=1, y=-1, id=50)

>(normal width/height) + (both error) + (id/no id)
(width = 2, height = 3, x=-1, y=-1, id=None)
(width = 2, height = 3, x=-1, y=-1, id=50)
# bad type
>(normal width/height) + (x error) + (id/no id)
(width = 2, height = 3, x="hello", y=0, id=None)
(width = 2, height = 3, x=(10,20), y=1, id=50)

>(normal width/height) + (y error) + (id/no id)
(width = 2, height = 3, x=0, y="hello", id=None)
(width = 2, height = 3, x=1, y=(10,20), id=50)

>(normal width/height) + (both error) + (id/no id)
(width = 2, height = 3, x=(10,20), y="hello", id=None)
(width = 2, height = 3, x="hello", y=(10,20), id=50)

                        # ~~~~~~~~~~ Area creation ~~~~~~~~~~ #
# ---------- Normal
>(big area)
(width = 100, height = 100)
>(small area)
(width = 1, height = 1)
>(extremely big area)
(width =  9223372036854775807, heigth =  9223372036854775807)
                        # ~~~~~~~~~~ Display creation ~~~~~~~~~~ #
# ---------- Normal

>(no coordinate)
r1 = Rectangle(2, 3, 0, 0)
r1.display()
r1 = Rectangle(2, 3)
r1.display()

>(coordinate)
r1 = Rectangle(2, 3, 0, 2)
r1.display()
r1 = Rectangle(2, 3, 2, 0)
r1.display()
r1 = Rectangle(2, 3, 2, 2)
r1.display()

